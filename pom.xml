<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <parent>
        <groupId>ch.emad.root</groupId>
        <artifactId>emad-root</artifactId>
        <version>0.1.1</version>
    </parent>

    <groupId>com.googlecode.madschuelerturnier</groupId>
    <artifactId>mad-schuelerturnier-parent</artifactId>
    <version>1.3.9</version>

    <packaging>pom</packaging>

    <properties>

        <emad-dropbox.version>0.0.20</emad-dropbox.version>
        <commons-httpclient.version>3.1</commons-httpclient.version>
        <tomcat-catalina.version>8.0.9</tomcat-catalina.version>
        <commons-email.version>1.3.2</commons-email.version>
        <biweekly.version>0.4.0</biweekly.version>
        <burlap.version>2.1.7</burlap.version>
        <hessian.version>4.0.38</hessian.version>

        <httpclient.version>4.3.6</httpclient.version>
        <openpojo.version>0.6.2</openpojo.version>

        <servlet-api.version>2.5</servlet-api.version>


        <hibernate-entitymanager.version>4.3.7.Final</hibernate-entitymanager.version>
        <hibernate-validator.version>5.2.0.Alpha1</hibernate-validator.version>
        <spring-security.version>4.0.0.RC1</spring-security.version>

        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <java.version>1.7</java.version>

    </properties>

    <modules>
        <module>mad-schuelerturnier-business</module>
        <module>mad-schuelerturnier-persistence</module>
        <module>mad-schuelerturnier-model</module>
        <module>mad-schuelerturnier-web</module>
