<?xml version="1.0" encoding="ISO-8859-1"?>
<workbook>
    <worksheet name="Tabelle 1">
        <section startRow="0" endRow="0">
        </section>
        <loop startRow="1" endRow="1" items="adressen" var="adresse"
              varType="com.googlecode.madschuelerturnier.model.Text">
            <section startRow="1" endRow="1">
                <mapping row="1" col="0">text.id</mapping>
                <mapping row="1" col="1">text.anrede</mapping>
                <mapping row="1" col="3">text.name</mapping>
                <mapping row="1" col="4">text.vorname</mapping>
                <mapping row="1" col="5">text.strasse</mapping>
                <mapping row="1" col="6">text.plz</mapping>
                <mapping row="1" col="7">text.ort</mapping>
                <mapping row="1" col="8">text.esr</mapping>
                <mapping row="1" col="9">text.anzahl</mapping>
                <mapping row="1" col="10">text.betrage</mapping>
                <mapping row="1" col="11">text.ressort</mapping>
                <mapping row="1" col="11">text.typ</mapping>
            </section>
            <loopbreakcondition>
                <rowcheck offset="0">
                    <cellcheck offset="0"/>
                </rowcheck>
            </loopbreakcondition>
        </loop>
    </worksheet>

</workbook>